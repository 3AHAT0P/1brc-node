{
  "extends": "@tsconfig/node21/tsconfig.json",
  "include": [
    "src/**/*"
  ],
  "exclude": [ ],
  "compilerOptions": {
    "types": [
      "node"
    ],
    "baseUrl": "./",
    "outDir": "./dist",
    "target": "ESNext",
    "preserveConstEnums": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": false
  }
}
